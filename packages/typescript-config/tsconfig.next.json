{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "moduleDetection": "force",
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "allowJs": true,
    "jsx": "preserve",
    "noUncheckedIndexedAccess": true,
    "plugins": [{ "name": "next" }]
  }
}
