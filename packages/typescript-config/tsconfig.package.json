{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "noEmit": true,
    "declarationDir": "dist/types",
    "tsBuildInfoFile": "dist/.tsbuildinfo"
  }
}
