{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "noEmit": false,
    "emitDeclarationOnly": false,
    "declaration": true,
    "declarationMap": true,
    "module": "ESNext",
    "target": "ES6",
    "moduleResolution": "node"
  }
}
